local orion = loadstring(game:HttpGet("https://raw.githubusercontent.com/LordNxqt/UiLibrary/refs/heads/main/Orion"))()

local winwd = orion:MakeWindow({Name = "arm pro gui", HidePremium = false, SaveConfig = true, ConfigFolder = "armprocfg"})

local maint = winwd:MakeTab({Name = "main", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local orbit = winwd:MakeTab({Name = "orbit", Icon = "rbxassetid://4483360751", PremiumOnly = false})
local telep = winwd:MakeTab({Name = "telep", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local aimn = winwd:MakeTab({Name = "aimn", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local misc = winwd:MakeTab({Name = "misc", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local setts = winwd:MakeTab({Name = "setts", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local pres = winwd:MakeTab({Name = "pres", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local vis = winwd:MakeTab({Name = "vis", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local auto = winwd:MakeTab({Name = "auto", Icon = "rbxassetid://4483345998", PremiumOnly = false})

local modex = "localx"
local svalv = 1
local obito = false
local oradr = 3
local orspd = 1
local obtar = nil
local tltrg = nil
local aimen = false
local aitrg = nil
local pulen = false
local plstr = 1
local trail = false
local trlen = 10
local autox = false
local intvl = 0.5
local lastt = tick()
local lockt = nil
local vison = false
local vislns = {}
local preset = "none"

local function pllst()
    local out = {}
    for _, p in ipairs(game.Players:GetPlayers()) do
        table.insert(out, p.Name)
    end
    return out
end

local function tgtls()
    local out = {}
    local lp = game.Players.LocalPlayer
    if modex == "localx" then
        if lp and lp.Character then
            table.insert(out, lp)
        end
    elseif modex == "other" then
        for _, p in ipairs(game.Players:GetPlayers()) do
            if p ~= lp and p.Character then
                table.insert(out, p)
            end
        end
    elseif modex == "allxx" then
        for _, p in ipairs(game.Players:GetPlayers()) do
            if p.Character then
                table.insert(out, p)
            end
        end
    end
    return out
end

local function sfire(chr, nam, cf)
    if not chr then
        return
    end
    local obj = chr:FindFirstChild(nam)
    if obj and obj.FireServer then
        pcall(function()
            obj:FireServer(cf)
        end)
    end
end

local function angch(plr)
    if not plr or not plr.Character then
        return
    end
    sfire(plr.Character, "ArmAngleChange", CFrame.new(1, .56, -svalv))
end

local function doang()
    for _, p in ipairs(tgtls()) do
        angch(p)
    end
end

local function orbup()
    if not obito or not obtar or not obtar.Character then
        return
    end
    local chr = obtar.Character
    local part = chr:FindFirstChild("HumanoidRootPart") or chr.PrimaryPart
    if not part then
        return
    end
    local start = tick()
    spawn(function()
        while obito and obtar and obtar.Character and obtar.Parent do
            local elp = tick() - start
            local ang = elp * orspd
            local x = math.cos(ang) * oradr
            local y = math.sin(ang * 0.5) * 0.5
            local z = math.sin(ang) * oradr
            local targcf = CFrame.new(part.Position) * CFrame.new(x, y + 2, z)
            sfire(obtar.Character, "ArmAngleChange", targcf)
            wait(1/60)
        end
    end)
end

local function telto(trg)
    if not trg or not trg.Character then
        return
    end
    sfire(trg.Character, "ArmAngleChange", trg.Character:GetPivot())
end

local function aimt()
    if not aimen or not aitrg or not aitrg.Character then
        return
    end
    local chr = aitrg.Character
    local hrp = chr:FindFirstChild("HumanoidRootPart")
    if not hrp then
        return
    end
    local lp = game.Players.LocalPlayer
    local tgtpos = lp.Character and lp.Character:FindFirstChild("Head") and lp.Character.Head.Position or hrp.Position
    local lookcf = CFrame.new(hrp.Position, tgtpos)
    sfire(aitrg.Character, "ArmAngleChange", lookcf)
end

local function pulsr()
    while pulen do
        for _, p in ipairs(tgtls()) do
            if p.Character then
                local cf = CFrame.Angles(0, math.sin(tick() * plstr) * .2, 0)
                sfire(p.Character, "ArmAngleChange", cf)
            end
        end
        wait(0.1)
    end
end

local function autoq()
    while autox do
        if tick() - lastt >= intvl then
            doang()
            lastt = tick()
        end
        wait(0.1)
    end
end

local function visd()
    for _, ln in ipairs(vislns) do
        if ln and ln.Parent then
            ln:Destroy()
        end
    end
    vislns = {}
    if not vison then
        return
    end
    for _, p in ipairs(game.Players:GetPlayers()) do
        if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
            local a = p.Character.HumanoidRootPart.Position
            local b = workspace.CurrentCamera.CFrame.Position
            local part = Instance.new("Part")
            part.Anchored = true
            part.CanCollide = false
            part.Size = Vector3.new(0.1, 0.1, (a - b).Magnitude)
            part.CFrame = CFrame.new(a, b) * CFrame.new(0, 0, -part.Size.Z/2)
            part.Material = Enum.Material.Neon
            part.Transparency = 0.6
            part.Parent = workspace
            table.insert(vislns, part)
        end
    end
end

local function locktgt()
    if lockt and lockt.Character then
        aitrg = lockt
        obtar = lockt
    end
end

maint:AddToggle({
    Name = "local",
    Default = true,
    Flag = "modloc",
    Callback = function(v)
        if v then
            modex = "localx"
            orion.Flags["modoth"] = false
            orion.Flags["modall"] = false
        end
    end
})

maint:AddToggle({
    Name = "other",
    Default = false,
    Flag = "modoth",
    Callback = function(v)
        if v then
            modex = "other"
            orion.Flags["modloc"] = false
            orion.Flags["modall"] = false
        end
    end
})

maint:AddToggle({
    Name = "allxx",
    Default = false,
    Flag = "modall",
    Callback = function(v)
        if v then
            modex = "allxx"
            orion.Flags["modloc"] = false
            orion.Flags["modoth"] = false
        end
    end
})

maint:AddSlider({
    Name = "sval",
    Min = 0,
    Max = 10,
    Default = 1,
    Flag = "svalf",
    Callback = function(v)
        svalv = v
    end
})

maint:AddToggle({
    Name = "auto",
    Default = false,
    Flag = "autof",
    Callback = function(v)
        autox = v
        if v then
            spawn(autoq)
        end
    end
})

maint:AddSlider({
    Name = "intvl",
    Min = 0.1,
    Max = 2,
    Default = 0.5,
    Flag = "intvf",
    Callback = function(v)
        intvl = v
    end
})

maint:AddButton({
    Name = "do angle",
    Callback = function()
        doang()
    end
})

orbit:AddDropdown({
    Name = "targ",
    Default = "none",
    Options = pllst(),
    Flag = "orbtarg",
    Callback = function(v)
        obtar = game.Players:FindFirstChild(v)
    end
})

orbit:AddSlider({
    Name = "radus",
    Min = 1,
    Max = 12,
    Default = 3,
    Flag = "oradr",
    Callback = function(v)
        oradr = v
    end
})

orbit:AddSlider({
    Name = "sped",
    Min = 0.1,
    Max = 5,
    Default = 1,
    Flag = "orspd",
    Callback = function(v)
        orspd = v
    end
})

orbit:AddToggle({
    Name = "enorb",
    Default = false,
    Flag = "enorb",
    Callback = function(v)
        obito = v
        if v then
            spawn(orbup)
        end
    end
})

orbit:AddButton({
    Name = "orbme",
    Callback = function()
        obtar = game.Players.LocalPlayer
        obito = true
        spawn(orbup)
    end
})

telep:AddDropdown({
    Name = "telto",
    Default = "none",
    Options = pllst(),
    Flag = "teltar",
    Callback = function(v)
        tltrg = game.Players:FindFirstChild(v)
    end
})

telep:AddButton({
    Name = "tel arm to tar",
    Callback = function()
        telto(tltrg)
    end
})

telep:AddButton({
    Name = "tel arm to me",
    Callback = function()
        telto(game.Players.LocalPlayer)
    end
})

aimn:AddDropdown({
    Name = "aimtar",
    Default = "none",
    Options = pllst(),
    Flag = "aimtar",
    Callback = function(v)
        aitrg = game.Players:FindFirstChild(v)
    end
})

aimn:AddToggle({
    Name = "enaim",
    Default = false,
    Flag = "enaim",
    Callback = function(v)
        aimen = v
        if v then
            spawn(function()
                while aimen do
                    aimt()
                    wait(0.1)
                end
            end)
        end
    end
})

aimn:AddButton({
    Name = "snap",
    Callback = function()
        aimt()
    end
})

misc:AddToggle({
    Name = "pulse",
    Default = false,
    Flag = "pulse",
    Callback = function(v)
        pulen = v
        if v then
            spawn(pulsr)
        end
    end
})

misc:AddSlider({
    Name = "plstr",
    Min = 0.1,
    Max = 5,
    Default = 1,
    Flag = "plstr",
    Callback = function(v)
        plstr = v
    end
})

