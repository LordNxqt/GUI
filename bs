    Name = "trail",
    Default = false,
    Flag = "trail",
    Callback = function(v)
        trail = v
    end
})

misc:AddSlider({
    Name = "trlen",
    Min = 2,
    Max = 30,
    Default = 10,
    Flag = "trlen",
    Callback = function(v)
        trlen = v
    end
})

misc:AddToggle({
    Name = "lock",
    Default = false,
    Flag = "lock",
    Callback = function(v)
        if v then
            lockt = game.Players.LocalPlayer
            locktgt()
        else
            lockt = nil
        end
    end
})

setts:AddToggle({
    Name = "vis",
    Default = false,
    Flag = "visf",
    Callback = function(v)
        vison = v
        visd()
    end
})

setts:AddButton({
    Name = "ref",
    Callback = function()
        visd()
    end
})

setts:AddSlider({
    Name = "global",
    Min = 0.5,
    Max = 3,
    Default = 1,
    Flag = "globl",
    Callback = function(v)
        orspd = orspd * v
    end
})

pres:AddDropdown({
    Name = "preset",
    Default = "none",
    Options = {"fast", "orbit", "aimbot", "combo", "chill"},
    Flag = "preset",
    Callback = function(v)
        preset = v
        if v == "fast" then
            modex = "allxx"
            svalv = 5
            orspd = 3
            intvl = 0.2
            autox = true
            aimen = true
            obito = true
        elseif v == "orbit" then
            obito = true
            oradr = 5
            orspd = 1.5
            modex = "other"
        elseif v == "aimbot" then
            aimen = true
            aitrg = game.Players.LocalPlayer
        elseif v == "combo" then
            autox = true
            aimen = true
            obito = true
            modex = "allxx"
        elseif v == "chill" then
            autox = false
            aimen = false
            obito = false
            svalv = 1
        end
    end
})

auto:AddToggle({
    Name = "autolock",
    Default = false,
    Flag = "autol",
    Callback = function(v)
        if v then
            spawn(function()
                while true do
                    locktgt()
                    wait(1)
                end
            end)
        end
    end
})

auto:AddButton({
    Name = "full go",
    Callback = function()
        autox = true
        aimen = true
        obito = true
        modex = "allxx"
    end
})

vis:AddToggle({
    Name = "showvis",
    Default = false,
    Flag = "showv",
    Callback = function(v)
        vison = v
        visd()
    end
})

vis:AddButton({
    Name = "clrvis",
    Callback = function()
        vison = false
        visd()
    end
})

game.Players.PlayerAdded:Connect(function(p)
    wait(1)
    visd()
end)

spawn(function()
    while true do
        if obito and obtar then
            orbup()
        end
        if trail then
            for _, p in ipairs(tgtls()) do
                if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    local hrp = p.Character.HumanoidRootPart
                    local pts = {}
                    table.insert(pts, hrp.Position)
                    for i = 1, trlen do
                        local off = Vector3.new(0, 0, 0)
                        local part = Instance.new("Part")
                        part.Anchored = true
                        part.CanCollide = false
                        part.Size = Vector3.new(0.15, 0.15, 0.15)
                        part.CFrame = CFrame.new(hrp.Position) * CFrame.Angles(0, 0, 0) * CFrame.new(0, -i * 0.2, 0)
                        part.Material = Enum.Material.Neon
                        part.Transparency = 0.7
                        part.Parent = workspace
                        game:GetService("Debris"):AddItem(part, 0.2)
                    end
                end
            end
        end
        if autox then
            if tick() - lastt >= intvl then
                doang()
                lastt = tick()
            end
        end
        if aimen then
            aimt()
        end
        wait(0.1)
    end
end)

orion:Init()
